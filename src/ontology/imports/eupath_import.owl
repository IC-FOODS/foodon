    <!-- http://purl.obolibrary.org/obo/IAO_0000104 -->

    <owl:Class rdf:about="&obo;IAO_0000104">
        <rdfs:subClassOf rdf:resource="&obo;IAO_0000033"/>
        <obo:IAO_0000111 xml:lang="en">plan specification</obo:IAO_0000111>
        <obo:IAO_0000114 rdf:resource="&obo;IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">A directive information entity with action specifications and objective specifications as parts that, when concretized, is realized in a process in which the bearer tries to achieve the objectives by taking the actions specified.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">OBI Plan and Planned Process branch</obo:IAO_0000119>
        <obo:IAO_0000119 xml:lang="en">OBI_0000344</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="&obo;eupath.owl"/>
        <rdfs:label xml:lang="en">plan specification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBI_0000272 -->

    <owl:Class rdf:about="&obo;OBI_0000272">
        <rdfs:subClassOf rdf:resource="&obo;IAO_0000104"/>
        <obo:IAO_0000111 xml:lang="en">protocol</obo:IAO_0000111>
        <obo:IAO_0000114 rdf:resource="&obo;IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">A plan specification which has sufficient level of detail and quantitative information to communicate it between investigation agents, so that different investigation agents will reliably be able to independently reproduce the process.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">OBI branch derived + wikipedia (http://en.wikipedia.org/wiki/Protocol_%28natural_sciences%29)</obo:IAO_0000119>
        <obo:IAO_0000412 rdf:resource="&obo;eupath.owl"/>
        <rdfs:label xml:lang="en">protocol</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ebi.ac.uk/efo/EFO_0002755 -->

    <owl:Class rdf:about="http://www.ebi.ac.uk/efo/EFO_0002755">
        <rdfs:subClassOf rdf:resource="&obo;OBI_0000272"/>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">Regular course of eating and drinking adopted by a person or animal. This does not include DIET THERAPY, a specific diet prescribed in the treatment of a disease.</obo:IAO_0000115>
        <obo:IAO_0000115 rdf:datatype="&xsd;string">The customary allowance of food and drink taken by a person or an animal from day to day, particularly one especially planned to meet specific requirements of the individual, including or excluding certain items of food; a prescribed course of eating and drinking in which the amount and kind of food, as well as the times at which it is to be taken, are regulated for therapeutic purposes or selected with reference to a particular state of health.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="&obo;eupath.owl"/>
        <rdfs:label rdf:datatype="&xsd;string">diet</rdfs:label>
    </owl:Class>
